{
  "name": "html-sass-webpack-boilerplate",
  "version": "1.0.0",
  "description": "html sass webpack boilerplate",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --open",
    "build": "webpack --mode=production",
    "format": "prettier --write src/js/**/* src/styles/**/*  src/templates/**/*",
    "serve": "serve dist"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format",
      "pre-push": "npm run format"
    }
  },
  "author": "Marius Jagminas",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "babel-loader": "^8.3.0",
    "css-loader": "^6.7.3",
    "file-loader": "^6.0.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^4.2.5",
    "mini-css-extract-plugin": "^2.7.2",
    "prettier": "2.0.5",
    "sass": "^1.26.5",
    "sass-loader": "^13.2.0",
    "serve": "^11.3.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {}
}
